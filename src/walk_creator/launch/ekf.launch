<?xml version="1.0"?>
<launch>

  <arg name="duration" default="20" doc="Duration in seconds"/>
  <arg name="auto_close" default="false" doc="Startup mode"/>
  <node name="ekf_control" pkg="ekf" type="ekf_controller.py" args="$(arg duration)" required="$(arg auto_close)" output="screen"></node>
  
</launch>